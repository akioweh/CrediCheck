{
  "name": "CrediCheck",
  "description": "Check the credibility of web pages",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
    }
  },
  "icons": {
  },
  "web_accessible_resources": [
    {
      "resources": [
        "api_client.js"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}
